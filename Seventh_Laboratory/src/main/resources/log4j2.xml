<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
<Appenders>
    <File name="explorationLogging" fileName="logs/exploration.log">
        <PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level %C - %msg%n"/>
    </File>
</Appenders>
<Loggers>
    <Root level="info">
        <AppenderRef ref="explorationLogging"/>
    </Root>
</Loggers>
</Configuration>